@section Styles {
    <link rel="stylesheet" href="~/css/admin.css" />
}

<div id="admin" class="bg-light">
    <div id="alertsRow" class="row bg-secondary text-light">
        <div class="col text-center">
            <div id="meetingStatus" class="alert alert-primary" role="alert">
                <h4 class="alert-heading">Meeting Status</h4>
            </div>
        </div>
        <div class="col text-center">
            <div id="socketStatus" class="alert alert-secondary" role="alert">
                <h4 class="alert-heading">Websocket Status</h4>
            </div>
        </div>
    </div>
    <div id="commandRow" class="row">
        <div class="col-lg-12 text-center">
        </div>
    </div>
    <div id="dataSpace" class="row justify-content-around"></div>
</div>


<script src="~/dist/admin.bundle.js" asp-append-version="true"></script>
